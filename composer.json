{
    "name": "vatradar/workers",
    "description": "Queue workers for crunching data",
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Workers\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "authors": [
        {
            "name": "VATRadar Devs",
            "email": "dev@vatradar.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-redis": "*",
        "influxdata/influxdb-client-php": "^3",
        "appwrite/appwrite": "^7",
        "bunny/bunny": "@dev",
        "vlucas/phpdotenv": "^5.5",
        "react/event-loop": "^1.3",
        "vatradar/dataobjects": "^0.1.0",
        "react/async": "^4.0",
        "react/promise": "^2.9",
        "jmikola/geojson": "^1.1",
        "tedivm/stash": "^0.17.6",
        "monolog/monolog": "^3.2",
        "clue/redis-react": "^3@dev",
        "guzzlehttp/guzzle": "^7.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "psy/psysh": "*",
        "phpstan/phpstan": "*",
        "phpstan/phpstan-deprecation-rules": "*",
        "phpunit/phpunit": "*",
        "phpstan/extension-installer": "*",
        "spaze/phpstan-disallowed-calls": "*",
        "phpstan/phpstan-phpunit": "*"
    },
    "config": {
        "bin-dir": "vbin",
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {"type": "composer", "url": "https://vatradar.repo.repman.io"}
    ]
}
