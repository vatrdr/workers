{
    "name": "vatradar/workers",
    "description": "Queue workers for crunching data",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "VATRadar Devs",
            "email": "dev@vatradar.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Workers\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Workers\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-redis": "*",
        "appwrite/appwrite": "^7",
        "bunny/bunny": "^0.5",
        "clue/redis-react": "^3@dev",
        "guzzlehttp/guzzle": "^7.5",
        "influxdata/influxdb-client-php": "^3",
        "jmikola/geojson": "^1.1",
        "monolog/monolog": "^3.2",
        "react/async": "^4.0",
        "react/event-loop": "^1.3",
        "react/promise": "^2.9",
        "tedivm/stash": "^0.17.6",
        "vatradar/dataobjects": "^0.7",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.0",
        "psy/psysh": "^0",
        "roave/security-advisories": "dev-latest",
        "spaze/phpstan-disallowed-calls": "^2.11"
    },
    "config": {
        "bin-dir": "vbin",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.7-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://vatradar.repo.repman.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
